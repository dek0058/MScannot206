@Component
script GameplayMapComponent extends Component

	@HideFromInspector()
	property Entity GameGroupEntity = nil

	@ExecSpace("ClientOnly")
	method void OnBeginPlay()
		self.GameGroupEntity = _EntityService:GetEntityByPath("/ui/GameGroup")
	end

	@ExecSpace("ClientOnly")
	@EventSender("LocalPlayer")
	handler HandleMapEnterEvent(MapEnterEvent e)
		_UserProxy:SetEnableCharacter(true)
		self.GameGroupEntity.Enable = true
	end

end