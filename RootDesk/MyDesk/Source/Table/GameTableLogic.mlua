	---@description "캐릭터 생성 신발 테이블"
	---@type CreateCharacterShoesTable
	property any createCharacterShoesTable = CreateCharacterShoesTable()

	---@description "캐릭터 생성 피부 테이블"
	---@type CreateCharacterSkinTable
	property any createCharacterSkinTable = CreateCharacterSkinTable()

	---@description "캐릭터 생성 테이블"
	---@type CreateCharacterTable
	property any createCharacterTable = CreateCharacterTable()
	
	-- 미니 게임
	---@description "클리커 몬스터 테이블"
	---@type ClickerMonsterTable
	property any clickerMonsterTable = ClickerMonsterTable()

	method void OnBeginPlay()
		self.itemTable:Load()
		self.characterEquipItemTable:Load()
		self.characterWeaponTable:Load()
		self.createCharacter1HWeaponTable:Load()
		self.createCharacter2HWeaponTable:Load()
		self.createCharacterSubWeaponTable:Load()
		self.createCharacterCapTable:Load()
		self.createCharacterCapeTable:Load()
		self.createCharacterCoatTable:Load()
		self.createCharacterEarAccTable:Load()
		self.createCharacterEarTable:Load()
		self.createCharacterEysAccTable:Load()
		self.createCharacterFaceAccTable:Load()
		self.createCharacterFaceTable:Load()
		self.createCharacterGloveTable:Load()
		self.createCharacterHairTable:Load()
		self.createCharacterLongCoatTable:Load()
		self.createCharacterPantsTable:Load()
		self.createCharacterShoesTable:Load()
		self.createCharacterSkinTable:Load()
		self.createCharacterTable:Load()
		
		-- 미니게임
		self.clickerMonsterTable:Load()

		self._T["init"] = true
	end

	method boolean IsInit()
		if self._T["init"] == nil then
			return false
		end
		return self._T["init"]
	end

end